<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>


<HBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.matthieubalmont.swimacrosslakestopwatch.controllers.CompetitionListCellController">
   <children>
      <Label fx:id="titleLabel">
         <HBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </HBox.margin>
      </Label>
      <Region HBox.hgrow="SOMETIMES" />
      <Button alignment="CENTER" maxWidth="100.0" minWidth="52.5" mnemonicParsing="false" onAction="#handleOpen" text="Open" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
         </HBox.margin>
      </Button>
      <Button alignment="CENTER" maxWidth="100.0" minWidth="52.5" mnemonicParsing="false" onAction="#handleDelete" text="Delete" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="10.0" right="15.0" top="10.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
